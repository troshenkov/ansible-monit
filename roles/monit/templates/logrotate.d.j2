# {{ ansible_managed }}

{% if item.path is defined %}
{% for path in item.path %}
{{ path }} {{ " " }}
{%- endfor -%}
{%- endif -%}{
{% if item.options is defined %}
{% for option in item.options %}
   {{ option }}
{% endfor %}
{% endif %}
{% if item.scripts is defined %}
{% for name, script in item.scripts.items() %}
   {{ name }}
   {{ script }}
{% endfor %}
{% endif %}
{% if item.postrotate is defined %}
{% for postrotate in item.postrotate %}
   postrotate
       {{ postrotate }}
{% endfor %}
{% endif %}
   endscript
}
